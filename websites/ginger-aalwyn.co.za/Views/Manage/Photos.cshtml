
@{
    Layout = null;
}
<div class="row">
    <h2 class="text-center" style=""display:inline-block">Manage Photos</h2>
     <button type="button" class="btn btn-success btn-lg btn-commit" ng-click="SaveChanges()">
      Save
    </button>
</div>
<div class="row">
    <div class="additem">
     <div class="span6">
         <label>Name</label><input type="text" ng-model="CurrenPhoto.Name" />
     </div>
         <div class="span6" style="margin-bottom:20px">
         <label>Upload</label><input type="file" class="btn btn-lg btn-default" ng-model="CurrenPhoto.RelativePath" />
          <input type="hidden" ng-model="CurrenPhoto.RelativePath" />
     </div>
   </div>
</div>
<div class="row">
    <div class="span8">
        <button type="button" class="btn btn-success btn-lg pull-right" ng-click="AddPerson()">Add</button>
    </div>
</div>