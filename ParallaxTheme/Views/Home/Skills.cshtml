@{
    ViewBag.Title = "Skills";
}
<h4>With almost 8 years in commercial software development, please see a breakdown of how I rate my skills</h4>
<div id="page-wrap">
    @Html.Partial("Skills/BusinessDevelopment")
    @Html.Partial("Skills/OtherSkills")
    @Html.Partial("Skills/OperatingSystems")
    @Html.Partial("Skills/Protocols")
    @Html.Partial("Skills/NetworkHardware")
    @Html.Partial("Skills/NetworkSoftware")
     @Html.Partial("Skills/Programming")
     @Html.Partial("Skills/Testing")
</div>
@section Styles
	{
    <link href="~/Content/progress/style.css" rel="stylesheet" />
}
@section Scripts
{
    <script>
        $(function () {
            $(".meter > span").each(function () {
                $(this)
                    .data("origWidth", $(this).width())
                    .width(0)
                    .animate({
                        width: $(this).data("origWidth")
                    }, 1200);
            });
        });
    </script>
 
}
